#ANSI 6429 constants:
black      30
red        31
green      32
yellow     33
blue       34
magenta    35
cyan       36
white      37
normal     00
brighten   01
underline  04
blink      05
background 07

escapes &Tbl{
	red        &red 
	green      &green &brighten &background : &normal &green
	blue       &blue &brighten : &normal &blue
	magenta    &magenta
	cyan       &cyan &brighten : &normal &cyan
}

